@model CadSeries.Models.Genero

@{
    ViewData["Title"] = "Delete";
}



<h3 class="alert-danger">Tem certeza que deseja deletar este cadastro?</h3>
<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <label>Genero</label>
            
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomeGenero)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Ativo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ativo)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Deletar" class="btn btn-danger" /> |
        <i class="fas fa-edit"></i> <a asp-action="Index">Voltar</a>
    </form>
</div>
